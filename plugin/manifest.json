{
  "name": "Recipes scrapper",
  "description": "Scrapper for recipes.skitek.dev",
  "version": "0.1",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Recipes",
    "default_icon": {
      "16": "images/icon16.png",
      "24": "images/icon24.png",
      "32": "images/icon32.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "24": "images/icon24.png",
    "32": "images/icon32.png",
    "128": "images/icon128.png"
  },
  "permissions": [
    "activeTab",
    "scripting"
  ]
}
